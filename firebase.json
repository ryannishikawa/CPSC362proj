{
  "hosting": {
    "target": "todoapp",
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "https://todoapp-mattdebinion-dev.web.app/:path*",
        "destination": "https://todoapp.mattdebinion.dev/:path",
        "type": 301
      },
      {
        "source": "https://todoapp-mattdebinion-dev.web.app",
        "destination": "https://todoapp.mattdebinion.dev/",
        "type": 301
      },
      {
        "source": "https://todoapp-mattdebinion-dev.firebaseapp.com/:path*",
        "destination": "https://todoapp.mattdebinion.dev/:path",
        "type": 301
      },
      {
        "source": "https://todoapp-mattdebinion-dev.firebaseapp.com",
        "destination": "https://todoapp.mattdebinion.dev/",
        "type": 301
      }
    ]
  }
}
